programa
{
	//Número 5.1
	
	funcao inicio()
	{
		inteiro n1, n2
		
		escreva("Digite o primeiro numero: ")
			leia(n1)
		escreva("Digite o segundo numero: ")
			leia(n2)
		
		escreva("Primeiro numero: ",n1)
		escreva("\nSegundo numero: ",n2)
		escreva("\nSoma: ",n1 + n2)
		escreva("\nSubtracao: ",n1 - n2)
		escreva("\nMultiplicacao: ",n1 * n2)
	}
}
